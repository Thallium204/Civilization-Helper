;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "civ_CMPVisual",
"language": "GDScript",
"path": "res://src/cmp/cmp_visual.gd"
} ]
_global_script_class_icons={
"civ_CMPVisual": ""
}

[application]

config/name="Civilization Helper"
run/main_scene="res://Menu.tscn"
config/icon="res://icon.png"

[autoload]

gl="*res://src/globals.gd"
data="*res://src/data.gd"
